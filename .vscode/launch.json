  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python Debugger: Module",
      "type": "debugpy",
      "request": "launch",
      "module": "backend.service"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Program",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}/frontend/src/backends2.ts",
      "preLaunchTask": "tsc: build - frontend/tsconfig.json",
      "outFiles": ["${workspaceFolder}/frontend/build/**/*.js"]
    }
  ]
}
